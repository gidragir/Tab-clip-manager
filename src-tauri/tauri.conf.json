{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "tab-clip-manager",
  "version": "0.1.0",
  "identifier": "com.tab-clip-manager.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "focus": true,
        "transparent": true,
        "width": 305,
        "height": 400,
        "resizable": false,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
