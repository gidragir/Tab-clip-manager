{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "updep": {},
    "lint": {},
    "lint:fix": {},
    "test": {
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["coverage/**"]
    }
  },
  "globalDependencies": [
    ".eslintignore",
    ".prettierignore",
    "tsconfig.base.json",
    "tsconfig.json"
  ]
}
